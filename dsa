#include<stdio.h>
int main()
{
    int i,j,n,sum=7;
    printf("Enter the size of an array");
    scanf("%d",&n);
    int a[n];
      printf("Enter the elements in the array");
    for(i=0;i<n;i++)
   {
        scanf("%d",&a[i]);
    }
    
    for(i=0;i<=n-1;i++)
    {
        for(j=i+1;j<n;j++){
        if(a[i]+a[j]==sum)
        {
            printf("%d%d",a[i],a[j]);
        }
       
    }
    }
    
    return 0;
}
